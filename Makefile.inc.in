CC=@CC@
RM=@RM@
MV=@MV@
CP=@CP@
INSTALL=@INSTALL@
PREFIX=@prefix@
CFLAGS=-O2 -g -Wall -DPREFIX=\"${PREFIX}\"
BIN=shrike@EXEEXT@
MKPASSWD=mkpasswd@EXEEXT@
LIBS=@LIBS@
LDFLAGS=@LDFLAGS@

# Debuggers need special flags - Note that a debugger will know
# how to modify these to get what he/she needs
CFLAGS += -ggdb -pedantic -Wno-long-long -Wshadow -Wmissing-declarations -Wmissing-prototypes
